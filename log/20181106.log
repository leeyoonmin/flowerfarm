          AA.ORDER_ID
        , AA.FORDER_ID
        , CONCAT(SUBSTR(AA.ORDER_TIME,1,4),'.',SUBSTR(AA.ORDER_TIME,5,2),'.',SUBSTR(AA.ORDER_TIME,7,2)) AS ORDER_TIME
        , AA.PRODUCT
        , AA.USER_ID
        , AA.IS_PAID
        , AA.IS_FORDER
        , AA.ORDER_STAT
        , AA.ORDER_STAT_NM
        , AA.PAY_PRICE
      	, BB.ID AS FORDER_ID
