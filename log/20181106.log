#SQL#Ledger_model->getMonthData #ByID leeyoonmin# [2018-11-06 11:39:21] 

      SELECT
      	  AA.USER_ID
      	, AA.TYPE_NM
      	, AA.TYPE_CD
      	, AA.DATE
      	, SUM(AA.AMOUNT) AS AMOUNT
        FROM(
      SELECT
      	  A.USER_ID
      	, '' AS TYPE_NM
      	, '02' AS TYPE_CD
      	, SUBSTR(A.ORDER_TIME,1,6) AS DATE
       	, SUM(B.ORDER_PRICE*B.ORDER_AMOUNT)
      	  +(CASE WHEN A.DELIVERY_TYPE = '01'
      	         THEN (SELECT CODE FROM COMMON_CODE_TB WHERE CODE_DV = '' AND CODE_NM = '')
      	         WHEN A.DELIVERY_TYPE = '02'
      	         THEN (SELECT CODE FROM COMMON_CODE_TB WHERE CODE_DV = '' AND CODE_NM = '')
      	         WHEN A.DELIVERY_TYPE = '03'
      	         THEN (SELECT CODE FROM COMMON_CODE_TB WHERE CODE_DV = '' AND CODE_NM = '')
      				ELSE 0 END) AS AMOUNT
      	, 'N' AS IS_LEDGER
        FROM ORDER_TB A
           , ORDER_ITEM_TB B
       WHERE A.ORDER_ID = B.ORDER_ID
         AND A.ORDER_STAT <> 99
         AND A.USER_ID = 'leeyoonmin'
         AND SUBSTR(A.ORDER_TIME,1,4) = '2018'
       GROUP BY SUBSTR(A.ORDER_TIME,1,6)
       UNION ALL
       SELECT
        A.USER_ID
        , (SELECT CODE_NM FROM COMMON_CODE_TB WHERE CODE_DV = '' AND CODE = A.TYPE) AS TYPE_NM
        , A.TYPE AS TYPE_CD
        , SUBSTR(A.DATE,1,6) AS DATE
        , SUM(A.AMOUNT) AS AMOUNT
        , 'Y' AS IS_LEDGER
        FROM LEDGER_TB A
           , LEDGER_CATE_TB B
        WHERE A.USER_ID = B.USER_ID
         AND A.TYPE = B.TYPE
         AND A.CATE = B.CATE_CODE
         AND SUBSTR(A.DATE,1,4) = '2018'
         AND A.USER_ID = 'leeyoonmin'
        GROUP BY A.USER_ID , A.TYPE, A.DATE
        ) AA
        WHERE 1=1
        GROUP BY TYPE_NM, TYPE_CD, DATE
        
