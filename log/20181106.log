                   WHEN A.DELIVERY_TYPE = '03'
                   THEN (SELECT CODE FROM COMMON_CODE_TB WHERE CODE_DV = '배송비' AND CODE_NM = '택배배송')
                ELSE 0 END) AS AMOUNT
          , 'N' AS IS_LEDGER
