              , CONCAT(C.PRODUCT_NAME,'  ', COUNT(1),'') AS PRODUCT
              , A.USER_ID
              , A.IS_PAID
              , A.IS_FORDER
              , A.ORDER_STAT
              , (SELECT CODE_NM FROM COMMON_CODE_TB WHERE WORK_DV = '' AND CODE_DV = '' AND CODE = A.ORDER_STAT) AS ORDER_STAT_NM
              , SUM(B.order_price * B.order_amount) AS PAY_PRICE
              FROM ORDER_TB A
